<+>/* Bertrand */\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: \"Cocogoose Condensed Trial\", sans-serif;\r\n}\r\n\r\nbody {\r\n    background-color: #1ACD77;\r\n}\r\n/* NAV */\r\n.navbar{\r\n    background-color: #1ACD77;\r\n}\r\n\r\n.navbar-brand{\r\n    width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.navbar-nav {\r\n    list-style-type: none;\r\n}\r\n\r\nul li a {\r\n    text-decoration: none;\r\n    color: #E6EAE5;\r\n    font-weight: bold;\r\n}\r\nul li a:hover{\r\n    color: #000000;\r\n}\r\n\r\n.logo_h1 {\r\n    font-family: Inter, sans-serif;\r\n    font-weight: bold;\r\n    font-size: 20px;\r\n    color: white;\r\n}\r\n\r\n.bg-color {\r\n    background-color: #1ACD77;\r\n    color: #ffff;\r\n}\r\n/* HEADER*/\r\n.header_h1 h1 {\r\n    font-size: 80px;\r\n    font-weight: bold;\r\n}\r\n.header_h1 p{\r\n    font-family: \"Inter Semi Bold\", sans-serif;\r\n}\r\n.header-p{\r\n    font-family: \"Inter Medium\", serif;\r\n    color: #ffffff;\r\n}\r\n.button {\r\n    color: #1ACD77;\r\n    background-color: #ffffff;\r\n    border: 1px solid #ffffff;\r\n    border-radius: 20px;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n}\r\n.button:hover{\r\n    background-color: #000000;\r\n    border: 1px solid #000000;\r\n}\r\n\r\n.img-fluid {\r\n    width: 100%;\r\n    height: auto;\r\n}\r\n/* MAIN */\r\n.bg-color-2{\r\n    background-color: #ffffff;\r\n}\r\n.img-fluid_2 {\r\n    width: 100%;\r\n    height: auto;\r\n    border-radius: 30px;\r\n}\r\n\r\n.button_2 {\r\n    color: #ffffff;\r\n    background-color: #1ACD77;\r\n    border: 1px solid #1ACD77;\r\n    border-radius: 20px;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n}\r\n.button_2:hover{\r\n    background-color: #000000;\r\n    color: #1ACD77;\r\n    border: 1px solid #000000;\r\n}\r\n\r\n.main-header h1 {\r\n    font-weight: bold;\r\n    color: #1ACD77;\r\n}\r\n.text-p{\r\n    font-family: \"Inter Medium\",sans-serif;\r\n    color: #000000;\r\n}\r\n.app_h1 h1{\r\n    font-size: 70px;\r\n    font-weight: bold;\r\n}\r\n.img-fluid_3{\r\n    width: 160px;\r\n    height: 50px;\r\n}\r\n\r\n@media (max-width: 992px){\r\n    .navbar{\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        z-index: 1;\r\n\r\n    }\r\n    .navbar-toggler{\r\n        color: #ffffff;\r\n    }\r\n    .navbar-collapse{\r\n        background-color: #1ACD77;\r\n        padding: 10px;\r\n    }\r\n    .navbar-nav{\r\n        display: none;\r\n        height: 1200px;\r\n    }\r\n    li a{\r\n        font-size: 30px;\r\n    }\r\n    li a img{\r\n        width: 40px;\r\n    }\r\n    header{\r\n        padding-top: 100px;\r\n    }\r\n    #main_formulier{\r\n        padding-top: 100px;\r\n    }\r\n}\r\n\r\n\r\n\r\n/* --- CONTACT_PAGINA --- */\r\n.contacteer-ons{\r\n    border: 1px solid transparent;\r\n    border-radius: 25px;\r\n}\r\n.map{\r\n    border: 1px solid transparent;\r\n    border-radius: 25px 0 0 25px;\r\n    z-index: 1;\r\n}\r\n\r\n.input{\r\n    border-bottom: 2px solid #000000;\r\n    border-top: none;\r\n    border-left: none;\r\n    border-right: none;\r\n}\r\n::placeholder{\r\n    color: #000000;\r\n    opacity: 100%;\r\n}
